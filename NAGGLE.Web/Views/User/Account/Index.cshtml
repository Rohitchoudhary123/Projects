@{
    Layout = "~/Views/Shared/_LayoutBusinessUser.cshtml";
}

<link href="~/Content/treePlug/zTreeStyle.css" rel="stylesheet" />
<link href="~/Content/jQueryAutocomplete/Autocomplete.css" rel="stylesheet" />
<link href="~/Content/Multiselect/multi-select.css" rel="stylesheet" />

<div id="mainContainer" class="display-none" ng-controller="userAccountController">
    <div id="AccountList" ng-show="CurrentMode==Mode.List">
        @{Html.RenderPartial("~/Views/User/Account/_AccountList.cshtml");}
    </div>

    <div id="AccountView" ng-show="CurrentMode==Mode.View">
        @{Html.RenderPartial("~/Views/User/Account/_AccountView.cshtml");}

    </div>
    <div id="AddEditAccount" ng-show="CurrentMode==Mode.AddEdit">
        @{Html.RenderPartial("~/Views/User/Account/_AddEditAccount.cshtml");}

    </div>
</div>

<script src="~/Scripts/treePlug/jquery.ztree.core-3.5.js"></script>
<script src="~/Scripts/treePlug/jquery.ztree.excheck-3.5.js"></script>
<script src="~/Scripts/jQueryAutocomplete/jquery.autocomplete.js"></script>
<script src="~/Scripts/treePlug/TreeView_AutoComplete.js"></script>
<script src="~/Scripts/bootstrap-multiselect.js"></script>
<script src="~/Scripts/App/Directives/Common.js"></script>
<script src="~/Scripts/App/Directives/treeView.js"></script>
